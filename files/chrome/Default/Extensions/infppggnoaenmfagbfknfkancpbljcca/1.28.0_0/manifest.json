{
   "background": {
      "scripts": [ "background.bundle.js" ]
   },
   "browser_action": {
      "default_icon": "icon-128.png",
      "default_popup": "popup.html"
   },
   "commands": {
      "element-picker": {
         "description": "Open element picker",
         "suggested_key": {
            "default": "Alt+P",
            "mac": "Alt+P"
         }
      },
      "open-dashboard": {
         "description": "Open the dashboard",
         "suggested_key": {
            "default": "Alt+A",
            "mac": "Alt+A"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "contentScript.bundle.js" ],
      "match_about_blank": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   }, {
      "all_frames": false,
      "js": [ "webService.bundle.js" ],
      "matches": [ "http://localhost/*", "*://*.automa.site/*", "*://automa.vercel.app/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' https://apis.google.com/ https://accounts.google.com/ https://www.googleapis.com https://ajax.googleapis.com; object-src 'self'",
   "description": "An extension for automating your browser by connecting blocks",
   "icons": {
      "128": "icon-128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Automa",
   "optional_permissions": [ "clipboardRead", "downloads", "contextMenus", "notifications", "cookies" ],
   "permissions": [ "tabs", "proxy", "alarms", "storage", "debugger", "\u003Call_urls>", "webNavigation", "unlimitedStorage" ],
   "sandbox": {
      "pages": [ "/sandbox.html" ]
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.28.0",
   "web_accessible_resources": [ "/elementSelector.css", "/Inter-roman-latin.var.woff2", "/icon-128.png", "/locales/*", "elementSelector.bundle.js" ]
}
